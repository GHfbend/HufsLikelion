<script>//화면 연습 중
    $(function(){
      $("#intro").hide();
      $('#roadmap').hide();
      $('#staff1').hide();
      $('#mem').hide();
      
      $(window).on('scroll' , function(){
        console.log($(window).scrollTop() );
        if($(window).scrollTop() > 90){
          $("#intro").addClass('animated fadeInLeft');
          $("#intro").show();
        }
        
        if($(window).scrollTop() > 850) {
          $('#roadmap').addClass('animated fadeInRight');
          $('#roadmap').show();
        }
        
        if($(window).scrollTop() > 1600) {
          $('#staff1').show();
          $('#staff1').addClass('animated fadeInDown');
        }
        
        if($(window).scrollTop() > 2300) {
          $('#mem').show();
          $('#mem').addClass('animated fadeInDown');
        }
      });
    });
</script>


